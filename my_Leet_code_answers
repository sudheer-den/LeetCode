function inserstionSort(arr) {
    for (let i = 0; i <= arr.length -1 ; i ++){
        for (let j = i + 1; j >= 0; j --){
            if (arr[j] > arr[j--]) {
                [arr[j], arr[j--]] = [arr[j--], arr[j]]
            }
        }
    }
    return arr
}

