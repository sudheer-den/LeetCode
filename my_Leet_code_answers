38. two sum
   //given array of integers return inices of two numbers that add up to given number

Ans:

function twoSum(arr, num) {

    for(var i = 0; i <= arr.length -1; i ++){
        for (var j = i+1; j <=arr.length -1; j++) {
            if (arr[i] + arr[j] === num) {
                return [i,j]
            }
        }
    }
    return null
}
var x = [1,2,3,5,15,34,21]
